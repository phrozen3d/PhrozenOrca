# Included by CMakeLists, edited by the build script
# (the version numbers are generated by the build script from the git current label)

set(SLIC3R_APP_NAME "PhrozenOrca")
set(SLIC3R_APP_KEY "PhrozenOrca")

if(NOT DEFINED BBL_RELEASE_TO_PUBLIC)
set(BBL_RELEASE_TO_PUBLIC "1")
endif()
if(NOT DEFINED BBL_INTERNAL_TESTING)
set(BBL_INTERNAL_TESTING "0")
endif()
set(Phrozen_VERSION "1.0.0")
string(REGEX MATCH "^([0-9]+)\\.([0-9]+)\\.([0-9]+)"
       Phrozen_VERSION_MATCH ${Phrozen_VERSION})
set(PHROZEN_ORCA_VERSION_MAJOR ${CMAKE_MATCH_1})
set(PHROZEN_ORCA_VERSION_MINOR ${CMAKE_MATCH_2})
set(PHROZEN_ORCA_VERSION_PATCH ${CMAKE_MATCH_3})

set(SLIC3R_VERSION "01.10.01.50")
